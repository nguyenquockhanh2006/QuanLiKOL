<!doctype html>
<html>
<head>
    <title>KhachHang</title>
    <link href="~/Context/css/khachhang.css" rel="stylesheet" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" href="//cdn.bootcss.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" defer></script>
    <script src="//cdn.bootcss.com/jquery/1.11.2/jquery.min.js" defer></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.12.0.min.js" defer></script>
    <script src="//cdn.bootcss.com/bootstrap/3.3.4/js/bootstrap.min.js" defer></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
    <script src="~/Context/js/Admin/CUS.js" defer></script>
    

</head>
<body>
    @{ 
        MXHvaQLKOL.Models.QuanLiKOLEntities db = new MXHvaQLKOL.Models.QuanLiKOLEntities();

        String accname = Session["id"].ToString();
        MXHvaQLKOL.Models.AccCustomer acccus = db.AccCustomers.Where(x => x.AccName == accname).FirstOrDefault();
        MXHvaQLKOL.Context.supportClass.XetMucHoanThanhHS xet = new MXHvaQLKOL.Context.supportClass.XetMucHoanThanhHS(acccus);     
        }
    <input type="text" id="trangthaiht" value="@xet.getht()" hidden/>
    <div id="menu">
        <div id="container">
            <div class="list"><a href="~/KhachHang/Index">TRANG CHỦ</a></div>
            <div class="list"><a href="~/Home/BangXepHang">BẢNG XẾP HẠNG</a></div>
            <div class="list"><a onclick="
    var ht = document.getElementById('trangthaiht').value;
    if (ht == 0) {
        alert('Vui lòng hoàn thành hồ sơ các nhân!');
        event.preventDefault();
    }
    " href="~/KhachHang/TaiHopDong">HỢP ĐỒNG</a></div>
            <div class="list"><a href="~/KhachHang/TimKiemKOL">TÌM KIẾM KOL</a></div>
            <div class="list"><a href="~/KhachHang/TrangCaNhanK">TRANG CÁ NHÂN</a></div>
            <div class="list"><a href="~/Login/Logout">ĐĂNG XUẤT</a></div>
        </div>
    </div>

    @RenderBody()

</body>
</html>
<script type="text/javascript">
    
</script>